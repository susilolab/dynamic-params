{
    "name": "susilolab/dynamic-params",
    "description": "Config params with editable in the admin panel for the Yii framework",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Agus Susilo",
            "email": "smartgdi@gmail.com",
            "homepage": "https://susilolab.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.*",
        "vitalets/x-editable-yii": "1.3.*",
        "yii2mod/yii2-editable": "1.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "susilolab\\systemparams\\": "/"
        }
    }
}